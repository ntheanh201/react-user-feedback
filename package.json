{
	"name": "@ntheanh201/react-user-feedback",
	"version": "0.0.7",
	"description": "Collect user's feedback",
	"repository": {
		"type": "git",
		"url": "https://github.com/ntheanh201/react-user-feedback.git"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./dist/styles.css": {
			"import": "./dist/styles.css",
			"require": "./dist/styles.css"
		}
	},
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "NODE_ENV=production rollup -c && npm run build-css",
		"build:storybook": "storybook build",
		"lint": "eslint . --ext .ts,.tsx,.js,.jsx",
		"prepare": "husky install",
		"storybook": "storybook dev -p 6006 --quiet",
		"test": "NODE_ENV=test jest",
		"test:coverage": "NODE_ENV=test jest --coverage",
		"test:watch": "NODE_ENV=test jest --watch",
		"changeset": "changeset",
		"release": "changeset publish",
		"start": "NODE_ENV=development parcel --no-cache demo/index.html",
		"build-css": "tailwindcss build src/global.css -o dist/styles.css"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{md,mdx,yml,json}": [
			"prettier --write"
		]
	},
	"browserslist": [
		"> 1%",
		"not dead",
		"not edge <= 18",
		"not ie 11",
		"not op_mini all"
	],
	"dependencies": {
		"@babel/runtime": "^7.20.7",
		"@hookform/resolvers": "^3.3.4",
		"@radix-ui/react-label": "^2.0.2",
		"@radix-ui/react-slot": "^1.0.2",
		"lucide-react": "^0.321.0",
		"react-hook-form": "^7.50.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@changesets/cli": "^2.26.0",
		"@commitlint/cli": "^17.4.1",
		"@commitlint/config-conventional": "^17.4.0",
		"@mdx-js/react": "^1.6.22",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.0.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-terser": "^0.3.0",
		"@storybook/addon-a11y": "^7.6.12",
		"@storybook/addon-actions": "^7.6.12",
		"@storybook/addon-essentials": "^7.6.12",
		"@storybook/addon-interactions": "^7.6.12",
		"@storybook/addon-links": "^7.6.12",
		"@storybook/addon-mdx-gfm": "^7.6.12",
		"@storybook/addon-styling-webpack": "^0.0.6",
		"@storybook/react": "^7.6.12",
		"@storybook/react-webpack5": "^7.6.12",
		"@storybook/testing-library": "^0.0.13",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@trivago/prettier-plugin-sort-imports": "^4.0.0",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"@zerollup/ts-transform-paths": "^1.7.18",
		"babel-loader": "^9.1.2",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"css-loader": "^6.10.0",
		"eslint": "8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-testing-library": "^5.9.1",
		"husky": "^8.0.3",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"jest-transform-stub": "^2.0.0",
		"lint-staged": "^13.1.0",
		"parcel": "^2.11.0",
		"postcss": "^8.4.33",
		"postcss-custom-properties": "^13.3.4",
		"postcss-import": "^16.0.0",
		"postcss-loader": "^8.1.0",
		"postcss-url": "^10.1.3",
		"prettier": "^2.8.2",
		"process": "^0.11.10",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rollup": "^2.79.1",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-typescript2": "^0.34.1",
		"storybook": "^7.6.12",
		"style-loader": "^3.3.4",
		"tailwind-merge": "^2.2.1",
		"tailwindcss": "^3.4.1",
		"tailwindcss-animate": "^1.0.7",
		"ts-jest": "^29.0.4",
		"tslib": "^2.4.1",
		"ttypescript": "^1.5.15",
		"typescript": "^4.9.4",
		"typescript-transform-paths": "^3.4.6",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"peerDependencies": {
		"react": "^16.8.0 || ^17 || ^18",
		"react-dom": "^16.8.0 || ^17 || ^18"
	},
	"engines": {
		"node": ">=16"
	}
}
